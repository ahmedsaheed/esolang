let Person = fn(name, age, occupation) {

  let person = {
    "name": name,
    "age": age,
    "occupation": {
        "title": occupation,
        "years": 5
    },
    "toString": fn() {
       return "Name: " + name + ", Age: " + age.to_string() + ", Occupation: " + occupation
     }
  }
  person
}

let p = Person("John", 30, "Software Developer")
println(p::age)
name := p::name
println(name)
println(p::occupation)
