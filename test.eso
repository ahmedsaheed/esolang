let fizzbuzz = fn(n, arr) {
  let i = 0
  when(i < n) {
    if (i % 15 == 0) { arr.append("FizzBuzz");}
    else {if (i % 3 == 0) {arr.append("Fizz");}
      else {if (i % 5 == 0) {arr.append("Buzz");}
        else {arr.append(i);}
      }}
      let i = i + 1
    }
    return arr
}

fizzbuzz(20, [])

